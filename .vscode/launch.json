{
  "version": "1.0.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "generate-node-api-ts",
      "protocol": "inspector",
      "cwd": "${workspaceRoot}",
      "env": {
        "NODE_ENV": "dev"
      },
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/ts-node-dev",
      "args": [
        "${workspaceRoot}/src/index.ts"
      ],
      "restart": true
    }
  ]
}